{
    "name": "gwinn/tinymce-fastload-bundle",
    "description": "This Bundle integrates in TinyMCE WYSIWYG editor fast images upload",
    "keywords": ["wysiwyg", "tinymce", "editor"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name":     "Alex Lushpai",
            "homepage": "http://lushpai.org",
            "email":    "alex@lushpai.org"
        }
    ],
    "require": {
        "php":             ">=5.3.0",
        "symfony/symfony": ">=2.1",
        "stfalcon/tinymce-bundle": "0.3.*"
    },
    "autoload": {
        "psr-0": {
            "Gwinn\\TinymceFastloadBundle": ""
        }
    },
    "target-dir" : "Gwinn/TinymceFastloadBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}




